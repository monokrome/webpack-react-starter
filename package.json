{
  "name": "webpack-react-starter",
  "version": "0.1.0",
  "description": "Minimal starter for using React + PostCSS with Webpack.",
  "keywords": [
    "webpack",
    "react"
  ],
  "author": "Bailey Stoner <monokrome@monokro.me>",
  "license": "MIT",
  "main": "lib/server.js",
  "scripts": {
    "start": "npm run server",
    "server": "BABEL_ENV=services nodemon --exec babel-node -e js,jsx,css,json -w src -w webpack.config.js -w .babelrc -w postcss.config.js src/services",
    "frontend": "webpack-dev-server --hot --inline"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.6",
    "babel-cli": "^6.23.0",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-plugin-css-modules-transform": "^1.2.1",
    "babel-plugin-tcomb": "^0.3.24",
    "babel-preset-es2015": "^6.22.0",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.22.0",
    "css-loader": "^0.26.2",
    "extract-text-webpack-plugin": "^2.0.0",
    "html-webpack-plugin": "^2.28.0",
    "isomorphic-fetch": "^2.2.1",
    "nodemon": "^1.11.0",
    "pleeease-filters": "^3.0.1",
    "postcss-calc": "^5.3.1",
    "postcss-color-function": "^3.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.2",
    "postcss-custom-selectors": "^3.0.0",
    "postcss-flexbugs-fixes": "^2.1.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.2",
    "postcss-math": "0.0.7",
    "postcss-media-minmax": "^2.1.2",
    "postcss-nesting": "^2.3.1",
    "postcss-selector-matches": "^2.0.5",
    "postcss-selector-not": "^2.0.0",
    "postcss-smart-import": "^0.6.9",
    "postcss-strip-units": "^1.2.0",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.13.2",
    "tcomb": "^3.2.17",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.4.1"
  },
  "dependencies": {
    "ejs": "^2.5.6",
    "chalk": "^1.1.3",
    "winston": "^2.3.1"
  }
}
